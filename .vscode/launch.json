    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Ejecutar __main__.py como m√≥dulo",
            "type": "python",
            "request": "launch",
            "module": "SatisPlanning",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src${pathSeparator}${env:PYTHONPATH}"
            }
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "justMyCode": true
        }
    ]
}